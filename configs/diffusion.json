{
  "training": {
    "observations": {
      "items": [
        {
          "name": "joint_values",
          "dimensions": 6,
          "history": 0
        },
        {
          "name": "end_effector_pose",
          "dimensions": 7,
          "history": 0
        },
        {
          "name": "target_pose",
          "dimensions": 7,
          "history": 0
        },
        {
          "name": "link_positions",
          "dimensions": 30,
          "history": 0
        },
        {
          "name": "pose",
          "dimensions": 7,
          "history": 0
        }
      ]
    },
    "eval": {
      "num_episodes": 10,
      "eval_frequency": 50,
      "eval_interval": 1000
    },
    "algo": "diffusionNet",
    "hyperparameters": {
      "tau": 0.001,
      "pi_lr": 0.0001,
      "lr_decay": true,
      "num_epochs": 100,
      "batch_size": 4096,
      "action_horizon": 1,
      "prediction_horizon": 16,
      "observation_horizon": 2
    },
    "network": {
      "actor": {
        "device": "cuda",
        "time_dim": 256,
        "max_action": 0.5,
        "n_timesteps": 100,
        "unet_layers": [256, 512, 1024],
        "beta_schedule": "squaredcos_cap_v2"
      }
    },
    "update_interval": 16000,
    "save_interval": 1,
    "num_processes": 1
  },
  "environment": {
    "render": false,
    "action_type": "delta",
    "min_arms_count": 1,
    "max_arms_count": 2,
    "task": {
      "generation_method": "random",
      "type": "static",
      "difficulty": {
        "min_distance_between_bases": 0.3,
        "max_distance_between_bases": 1.0
      }
    },
    "arms_position_picker": "evenly_spaced",
    "retry_on_fail": false,
    "workspace_radius": 0.85,
    "reward": {
      "collision_penalty": -0.05,
      "individually_reach_target": 0.01,
      "collectively_reach_target": 1.0
    },
    "terminate_on_collision": true,
    "terminate_on_collectively_reach_target": true,
    "episode_length": 1000,
    "action_interval": 0.05,
    "collision_distance": 0.0,
    "stop_arm_after_reach": false,
    "arm_type": "UR5",
    "arm_speed": 0.02,
    "position_tolerance": 0.01,
    "orientation_tolerance": 0.05
  }
}
